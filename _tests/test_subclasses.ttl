@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/ex#> .

prov:Plan rdfs:subClassOf prov:Entity .

ex:SoftwareCode rdfs:subClassOf prov:Plan .

<http://example.com/dataset/a> a prov:Plan ;
    rdfs:label "Plan A" .

<http://example.com/dataset/b> a ex:SoftwareCode ;
    rdfs:label "Code B" .

# no label
<http://example.com/dataset/c> a ex:SoftwareCode .